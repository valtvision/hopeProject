function SendOtps2(mod) {
        var mob = document.getElementById("txtPhoneCon").value;
		var otp = document.getElementById("otpids2").value;
        //var digit1 = document.getElementById("digit1").value;
        //var digit2 = document.getElementById("digit2").value;
        //var digit3 = document.getElementById("digit3").value;
        //var digit4 = document.getElementById("digit4").value;
        //var otp = digit1+digit2+digit3+digit4;
      var xhttp = new XMLHttpRequest();
      
      xhttp.onreadystatechange = function() {

      if (this.readyState == 4 && this.status == 200) {
            //alert(this.responseText);
            if(this.responseText=="OTP verified success"){
                document.getElementById("setstatus").innerHTML = '<span><img class="varify-icon-width" src="https://aaft.com/assets/Images/green-verified-.png">Verified</span>';
                document.getElementById("digithide01").style.display = "none";
                document.getElementById("digithide02").style.display = "none";
                document.getElementById("mobvery").innerHTML = '<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Click to Continue</span></button>';
                 
            }
          document.getElementById("otpmsgs2").innerHTML = this.responseText;
        }
      };
      //xhttp.open("GET", "http://127.0.0.1:8000/assets/number_varification.php?mode="+mod+"&mobile="+mob+"&otp="+otp, true); //for local
      xhttp.open("GET", "https://aaft.com/assets/number_varification.php?mode="+mod+"&mobile="+mob+"&otp="+otp, true); //for server
      xhttp.send();
      //alert("https://aaft.com/assets/number_varification.php?mode="+mod+"&mobile="+mob+"&otp="+otp);
    }
